{
  "name": "Gruber Dark",
  "colors": {
    "editor.lineHighlightBackground": "#65737e30",
    "editor.background": "#1F212A",
    "editorCursor.foreground": "#c0c5ce",
    "editor.selectionBackground": "#343d46",
    "editor.foreground": "#c0c5ce",
    "editorWhitespace.foreground": "#65737e",
    "editorIndentGuide.background": "#3b5364",
    "sideBar.background": "#1c1f26",
    "activityBar.background": "#1c1f26",
    "sideBarSectionHeader.background": "#1c1f26",
    "editorGroupHeader.tabsBackground": "#1c1f26",
    "editorGroup.dropBackground": "#1c1f26",
    "tab.border": "#1c1f26",
    "tab.inactiveForeground": "#65737f",
    "tab.inactiveBackground": "#1c1f26",
    "statusBar.background": "#1c1f26",
    "statusBar.foreground": "#65737f",
    "statusBar.debuggingBackground": "#F03A47",
    "statusBar.debuggingForeground": "#F5FAFF",
    "statusBar.noFolderBackground": "#7e7edd",
    "statusBar.noFolderForeground": "#F5FAFF",
    "titleBar.inactiveBackground": "#1c1f26",
    "titleBar.activeBackground": "#1c1f26",
    "list.activeSelectionBackground": "#343d46",
    "list.inactiveSelectionBackground": "#343d46",
    "list.focusBackground": "#343d46",
    "list.hoverBackground": "#1c1f26",
    "input.background": "#1c1f26",
    "dropdown.background": "#1c1f26",
    "editorWidget.background": "#232830",
    "editorHoverWidget.background": "#232830",
    "editorHoverWidget.border": "#343d46",
    "diffEditor.insertedTextBackground": "#43d08a10",
    "diffEditor.removedTextBackground": "#e052521a",
    "editorGutter.modifiedBackground": "#4f71ff",
    "editorGutter.addedBackground": "#4FFF95",
    "debugToolBar.background": "#1F212A",
    "debugIcon.stepIntoForeground": "#85b1e0",
    "debugIcon.stepOutForeground": "#85b1e0",
    "debugIcon.continueForeground": "#85b1e0",
    "debugIcon.stepOverForeground": "#85b1e0",
    "debugIcon.pauseForeground": "#85b1e0",
    "debugIcon.disconnectForeground": "#F03A47",
    "debugIcon.stopForeground": "#F03A47",
    "editor.stackFrameHighlightBackground": "#343d46",
    "debugIcon.breakpointCurrentStackframeForeground": "#BDC5E8",
    "debugIcon.breakpointForeground": "#F03A47",
    "editorRuler.foreground": "#2D303A",
    "editorWarning.foreground": "#eddfc0",
    "editorError.foreground": "#F03A47",
    "editorInfo.foreground": "#85b1e0",
    "notificationsInfoIcon.foreground": "#9CDCFE",
    "notificationsWarningIcon.foreground": "#eddfc0",
    "notificationsErrorIcon.foreground": "#F03A47",
    "editor.findMatchBackground": "#4f5b66",
    "editor.findMatchHighlightBackground": "#4f5b6699",
    "editor.foldBackground": "#4f5b6620"
  },
  "tokenColors": [
    {
      "name": "Brackets",
      "scope": ["punctuation.brackets.curly"],
      "settings": {
        "foreground": "#dddddd"
      }
    },
    {
      "name": "Comment",
      "scope": [
        //base (works with rust)
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6b6b6b"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        //base (works with rust)
        "keyword.control",
        "keyword.other",
        "keyword.operator.new",
        "storage.modifier",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators, symbols and builtin types",
      "scope": [
        //base (works with rust)
        "keyword.operator",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.logical",
        "entity.name.type.primitive",
        "entity.name.type.numeric",
        //java additions
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": ""
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#73c936"
      }
    },
    {
      "name": "Constants, numbers and internals",
      "scope": [
        //base (works with rust)
        "constant",
        //java additions
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#9e95c7"
      }
    },
    {
      "name": "self and super",
      "scope": [
        //base (works with rust)
        "variable.language.self",
        "variable.language.super",
        //java additions
        "variable.language.this",
        //python additions
        "variable.language.special.self",
        "variable.parameter.function.language.special.self",
        "variable.language.special.cls",
        "variable.parameter.function.language.special.cls"
      ],
      "settings": {
        "foreground": "#73c936",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Metaprogramming",
      "scope": [
        "meta.attribute",
        "meta.macro",
        //julia additions
        "support.function.macro"
      ],
      "settings": {
        "foreground": "#00d5ff"
      }
    },
    {
      "name": "Types",
      "scope": [
        //General
        "entity.name.type",
        //java additions
        "entity.other.inherited-class",
        "storage.type.java",
        "storage.type.generic.java",
        "storage.type.object.array.java",
        //julia additions
        "support.type.julia"
      ],
      "settings": {
        "foreground": "#dddddd",
        "fontStyle": "underline"
      }
    },
    /*
		[RUST-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "Rust metavariable",
      "scope": "variable.other.metavariable.name",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Rust metavariable type identifier",
      "scope": "variable.other.metavariable.specifier",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": ""
      }
    },
    {
      "name": "Rust Result",
      "scope": ["entity.name.type.result", "entity.name.type.option"],
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Rust lifetime",
      "scope": "entity.name.type.lifetime",
      "settings": {
        "foreground": "#9e95c7",
        "fontStyle": ""
      }
    },
    /*
		[PYTHON-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "Python magic",
      "scope": [
        "support.variable.magic.python",
        "support.function.magic.python"
      ],
      "settings": {
        "foreground": "#00d5ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python function decorators",
      "scope": "meta.function.decorator.python",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
    /*
		[C/C++-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "C/C++ hex notation fix",
      "scope": "keyword.other.unit.hexadecimal",
      "settings": {
        "foreground": "#9e95c7",
        "fontStyle": ""
      }
    },
    {
      "name": "C/C++ builtin types fix",
      "scope": "storage.type.built-in.primitive.c",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": ""
      }
    },
    /*
		[JAVA-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "Import fix",
      "scope": [
        "storage.modifier.import.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#dddddd",
        "fontStyle": ""
      }
    },
    /*
		[MARKUP-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "Markup headers",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#9e95c7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#9e95c7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": ""
      }
    },
    /*
		[JSON-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "JSON property name",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#ffd900"
      }
    },
    /*
		[XML-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "XML property name",
      "scope": "meta.tag.xml",
      "settings": {
        "foreground": "#ffd900"
      }
    },
    /*
		[TOML-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "TOLM table tag",
      "scope": "meta.tag.table.toml",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
    /*
		[LUA-SPECIFIC MODIFICATIONS]
	*/
    {
      "name": "LUA keyword local",
      "scope": "keyword.local.lua",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },

    // Golang

    {
      "name": "Go function decorators",
      "scope": "keyword.function.go",
      "settings": {
        "foreground": "#ffd900",
        "fontStyle": "bold"
      }
    },
  ]
}
